library(dplyr)
library(readr)
library(haven)
library(data.table)
supp = read_sas('~/Data/BloodProduct/NSQIP_raw/transfusion2019_puf.sas7bdat')
df = read_csv('~/Data/BloodProduct/puf19_lite.csv') %>%
left_join(supp %>%
mutate(CASEID = caseid) %>%
select(CASEID, bleed_units_tot)
) %>%
mutate(bleed_units_tot = na_if(bleed_units_tot, -99))
